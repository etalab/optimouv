                     $("#contrainteInterdiction").click(function(){
                         $("#contrainteInterdictionBody").toggle();
                     });
                 });

                 $(document).ready(function(){
                     $("#contrainteRepartition").click(function(){
                         $("#contrainteRepartitionBody").toggle();
                     });
                 });

