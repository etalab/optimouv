                                                                         var marker = 'marker' + incrment;
                                                                         window[marker] = new H.map.Marker({
                                                                             lat: parseFloat(x),
                                                                             lng: parseFloat(y)
                                                                         }, {icon: window[icon]}  );


                                                                          points.push(window[marker]);

                                                                         var incrment = incrment + 1;

                                                                         {% endfor %}
