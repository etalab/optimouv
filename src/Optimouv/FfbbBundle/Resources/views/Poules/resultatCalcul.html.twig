                                                                 //construction des points de differentes villes
                                                                 function addMarkersAndSetViewBounds() {
                                                                     var incrment = 1;
                                                                     var points = [];
                                                                     var incrementIcons = 0;

                                                                     {%  for coordPoule in coordPoules %}

