{% extends '::BOLayout.html.twig' %}

 {% block pagewrapper %}

     <div id="page-wrapper">

         <div class="row">
             <div class="col-lg-12">
                 <h3 class="page-header">Listes de participants</h3>
             </div>
         </div>


         <div id="div_participants" class="col-md-6">
             <input class="filter form-control" type="text" placeholder="Rechercher parmi mes listes de participants">
             <select multiple="multiple" id="liste_partcipants" class="form-control" style="height: 100px;">
                 {% for ListeParticipants in listesParticipants %}
                     <option	value={{ ListeParticipants.id }}> {{ ListeParticipants.nom }}</option>
                 {% endfor %}
             </select>
             <br>
             <input id="input_liste_participants" type="file" class="btn pull-left" style="width:30%">
             <button id="btn_import_liste_participants" type="submit" class="btn btn-default  pull-left">Importer une nouvelle liste </button>

         </div>


         <div class="panel-group col-md-6" id="panel_statut_upload_liste_participants">
             <div class="panel panel-default">
                 <div class="panel-heading">
                     <h4 class="panel-title">
                         <a data-toggle="collapse" href="#collapse_statut_upload_participants">Statut d'upload</a>
                     </h4>
                 </div>
                 <div id="collapse_statut_upload_participants" class="panel-collapse collapse">
                     <div class="panel-body" id="msg_upload_liste_participants">Veuillez uploader un fichier csv</div>
                 </div>
             </div>
         </div>




         <div class="row">
             <div class="col-lg-12">
                 <h3 class="page-header">Listes de lieux </h3>
             </div>
         </div>

         <div id="div_lieux" class="col-md-6">
             <input class="filter form-control" type="text" placeholder="Rechercher parmi mes listes de lieux">
             <select multiple="multiple" id="liste_lieux" class="form-control" style="height: 100px;">
                 {% for listeLieux in listesLieux %}
                     <option	value={{ listeLieux.id}}>{{ listeLieux.nom }}</option>
                 {% endfor %}
             </select>
             <br>
             <input id="input_liste_lieux" type="file" class="btn pull-left" style="width:30%">
             <button id="btn_import_liste_lieux" type="submit" class="btn btn-default  pull-left">Importer une nouvelle liste </button>
         </div>


         <div class="panel-group col-md-6" id="panel_statut_upload_liste_lieux">
             <div class="panel panel-default">
                 <div class="panel-heading">
                     <h4 class="panel-title">
                         <a data-toggle="collapse" href="#collapse_statut_upload_lieux">Statut d'upload</a>
                     </h4>
                 </div>
                 <div id="collapse_statut_upload_lieux" class="panel-collapse collapse">
                     <div class="panel-body" id="msg_upload_liste_lieux">Veuillez uploader un fichier csv</div>
                 </div>
             </div>
         </div>


    </div>


 {% endblock %}

{% block javascripts %}
{% endblock %}





