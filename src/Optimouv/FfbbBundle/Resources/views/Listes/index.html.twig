                 <input class="filter form-control" type="text" placeholder="Rechercher parmi mes listes de lieux">
                 <select multiple="multiple" id="liste_lieux" class="form-control" style="height: 100px;"></select>
             </div>




        </div>









         {% block javascripts %}
