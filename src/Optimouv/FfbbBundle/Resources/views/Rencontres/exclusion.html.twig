{% extends '::BOLayout.html.twig' %}

{% block pagewrapper %}

        <div id="page-wrapper">

            <div class="row">
                <div class="bs-example">
                    <ul class="breadcrumb">
                        <li><a href="{{ path('ffbb_accueil_connect') }}">Accueil</a></li>
                        <li><a href="{{ path('ffbb_choisir_liste_participants') }}">Choix de listes</a></li>
                        <li><a href="{{ path('ffbb_choisir_groupe', {'idListe': idListe}) }}">Choix de groupes</a></li>
                        <li><a href="{{ path('ffbb_villes_groupe', {'idGroupe': idGroupe}) }}">Participants</a></li>

                        <li class="active">Exclusion de zones géographiques</li>
                    </ul>
                </div>
                <div class="col-lg-12">
                    <h3 class="page-header">Choisissez un type de lieu </h3>
                    <h4>{{ nomListe }} | {{ nomGroupe }}</h4>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-4">
                    <div class="panel panel-red" style="resize:vertical; overflow-y:auto;">
                        <div class="panel-heading">
                            Participants
                        </div>
                        <div class="panel-body" style="min-height: 150px; max-height: 190px">
                            <ul class="list-unstyled" style="padding-left:20px; list-style-image: url('{{ asset('bundles/ffbb/images/puce-mini.png') }}')">
                                 {% for participant in participants %}
                                    <li> {{ participant.ville|upper }}</li>
                                {% endfor %}
                            </ul>
                        </div>

                    </div>
                </div>
                <!-- /.col-lg-4 -->

                <div class="col-lg-4">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            Type de lieu
                        </div>

                        <form style="padding-left:20px;" action="{{ path('ffbb_lancer_producer', {'idGroupe':idGroupe}) }}" method="POST" onsubmit="msgLancement()">

                            <div class="radio">
                                <label>
                                    <input type="radio" name="rencontre" id="meilleurLieuOption" value="meilleurLieu" required>
                                    Lieux définis
                                </label>
                            </div>
                            <div class="radio">
                                <label>
                                    <input type="radio" name="rencontre" id="BarycentreOption" value="barycentre" disabled>
                                    Barycentre
                                </label>
                            </div>

                            <button type="submit" class="btn btn-default-rencontre  pull-right" >Lancer le calcul
                            </button>


                        </form>


                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            Ajouter des contraintes
                        </div>
