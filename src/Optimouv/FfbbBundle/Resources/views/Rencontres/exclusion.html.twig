                            <div style="padding-top: 10px">
                                Saisissez ici le nombre d'habitants maximum des zones à exclure.

                            </div>
                            <div class="form-group">
                                {#<label>Text Input with Placeholder</label>#}
                                <input name="valeurExclusion" id="valeurExclusion" type="number" class="form-control" min="1000" value="{{ valeurExclusion }}" required>
                            </div>
                            <input type="hidden" name="rencontre" value="exclusion">
                            <button type="submit" class="btn btn-default-rencontre  pull-right" >Lancer le calcul
                            </button>
                        </form>

                    </div>
                </div>


